{
  "name": "composers",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "server": "node server/soundcloudProxy.js",
    "timeline:build": "node scripts/buildTimeline.js",
    "timeline:watch": "chokidar knightlab-timeline/composers.md knightlab-timeline/timeline.source.json -c \"npm run timeline:build\" --initial",
    "build": "npm run timeline:build && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "deploy": "npm run build && npm run deploy:rsync",
    "deploy:rsync": "rsync -az --delete --no-perms --no-owner --no-group -e \"ssh -i ~/.ssh/default_my\" dist/ root@139.59.136.231:/composers/",
    "deploy:backend": "rsync -az --delete --no-perms --no-owner --no-group -e \"ssh -i ~/.ssh/default_my\" server/ root@139.59.136.231:/var/www/composers-backend/server/ && ssh -i ~/.ssh/default_my root@139.59.136.231 'cd /var/www/composers-backend && pm2 restart composers-soundcloud'"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.5.0",
    "globals": "^16.4.0",
    "marked": "^17.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "3.6.2",
    "vite": "^7.1.11",
    "vite-plugin-full-reload": "^1.2.0",
    "vite-plugin-vue-devtools": "^8.0.3"
  }
}
